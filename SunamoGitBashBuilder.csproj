<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>24.2.1.3</Version>
    <Description>Generator of git commands</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SunamoArgs" Version="24.2.1.3" />
    <PackageReference Include="SunamoColors" Version="24.2.1.1" />
    <PackageReference Include="SunamoDelegates" Version="24.2.1.1" />
    <PackageReference Include="SunamoLogger" Version="24.2.1.1" />
    <PackageReference Include="SunamoStringGetLines" Version="24.2.1.2" />
    <PackageReference Include="SunamoTwoWayDictionary" Version="24.2.1.1" />
    <PackageReference Include="SunamoTypeOfMessage" Version="24.2.1.1" />
  </ItemGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
</Project>